import '@testing-library/jest-dom/extend-expect';

import React from 'react'; 
import { render } from '@testing-library/react'; 
import '@testing-library/jest-dom/extend-expect' 

describe('checked {{ pascalCase name }}', () => {
    it('should render good {{ pascalCase name }}Container', () => {
        const { getByText } = render(< {{ pascalCase name }}Container />);
        const element = getByText(/component/i);

        expect(element).toBeInTheDocument();
    })

    it('should math snapshot {{ pascalCase name }}Container', () => {
        const { getByText } = render(< {{ pascalCase name }}Container />);
        const element = getByText(/component/i);

        expect(element).toMatchSnapshot();
    })
});